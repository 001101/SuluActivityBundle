<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="massive_activity.admin.class">Massive\Bundle\ActivityBundle\Admin\MassiveActivityAdmin</parameter>
        <parameter key="massive_activity.admin.content_navigation.class">Massive\Bundle\ActivityBundle\Admin\MassiveActivityContentNavigation</parameter>
    </parameters>
    <services>
        <service id="massive_activity.admin" class="%massive_activity.admin.class%">
            <argument>%sulu_admin.name%</argument>

            <tag name="sulu.admin"/>
            <tag name="sulu.context" context="admin"/>
        </service>
        <service id="massive_activity.admin.content_navigation" class="%massive_activity.admin.content_navigation.class%">
            <tag name="sulu.contact.admin.content_navigation"/>
            <tag name="sulu.context" context="admin"/>
        </service>
    </services>
</container>
